{
    "version": 2,
    "builds": [
      {
        "src": "python_compiler/wsgi.py",  
        "use": "@vercel/python" ,
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "python_compiler/wsgi.py"
      }
    ]
  }
  